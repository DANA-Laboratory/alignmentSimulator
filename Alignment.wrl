DEF ScriptAnimationControl Script 
  {
  eventIn SFTime toggle
  eventIn SFBool activated
  eventIn SFTime cycle
  eventOut SFTime triggerStart
  eventOut SFTime triggerStop
  eventOut SFTime triggerStartInt
  directOutput TRUE
  field SFNode tproc   USE timer
  field SFBool state FALSE
  field SFTime pause 0
  url 
    [
    "vrmlscript:
 function activated( active, curtime ) 
 {
  if( active ) {
   triggerStart = curtime;
   state = TRUE;
  }
  else {
   triggerStop = curtime;
   state = FALSE;
  }
 }
 function cycle( curtime ) {
  triggerStartInt = curtime;
 }
 function toggle( stime ) {
  if( state )
  {
   state = FALSE;
   pause = stime - tproc.startTime_changed;
   tproc.enabled = FALSE;
  }
  else
  {
   state = TRUE;
   tproc.set_startTime = stime - pause;
   tproc.enabled = TRUE;
   pause = 0;
  }
 }
 "
    ]
  }

ROUTE TouchS.touchTime TO ScriptAnimationControl.toggle
ROUTE timer.isActive TO ScriptAnimationControl.activated
ROUTE timer.cycleTime TO ScriptAnimationControl.cycle
ROUTE DriverClockClamprotation.value_changed TO nghalh_BoxTr.set_rotation


